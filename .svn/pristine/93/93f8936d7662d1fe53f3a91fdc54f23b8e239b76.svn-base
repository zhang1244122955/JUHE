.pay-verify .pv-line {
line-height: 28px;
margin-bottom: 10px;
}
.pay-verify .pv-line.pv-line-shortPwd .pl-s-title {
    color: #999;
    line-height: 20px;
    margin-bottom: 5px;
}
.ui-shortPwd {
    display: inline-block;
    width: 235px;
    height: 25px;
    padding: 5px 0;
    line-height: 20px;
    font-size: 12px;
    border: 1px solid #c6c6c6;
    border-radius: 2px;
    -webkit-box-shadow: 0 1px 1px 1px #ededed inset;
    box-shadow: 0 1px 1px 1px #ededed inset;
    vertical-align: middle;
    overflow: hidden;
    -o-transition: border-color .1s;
    -webkit-transition: border-color .1s;
    transition: border-color .1s;
    background: url(i/shortPwd-bg.png) center center no-repeat #fff;
}
.ui-shortPwd .ui-shortPwd-input {
    border: 0;
    padding: 0;
    width: 34px;
    height: 24px;
    line-height: 24px;
    float: left;
    background: 0 0;
    margin: 0 2px;
    text-align: center;
    font-size: 16px;
    border-right:1px solid grey;
}
