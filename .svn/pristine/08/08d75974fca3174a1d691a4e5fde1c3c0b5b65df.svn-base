	height: 100px;
	padding-top: 40px;
}
.reg_row{
	
	width: 400px;
	height: 70px;
	position: relative;
	//color: #00bdff;
	font-size: 14px;
	//border: 1px solid;
	color: #ABC2E8;
	font-size: 12px;
}
