.reg_row input{
	text-indent: 1em;
	width: 400px;
	height: 35px;
	border: 1px solid #c5c5c5;
	border-radius: 3px;
}
.error{
	display: none;
	width: 200px;
	height: 20px;
	//border: 1px solid;
	position: absolute;
	left: 410px;
	top: 5px;
	color: #f15b5b;
}
.error_row{
	font-size: 12px;
}
